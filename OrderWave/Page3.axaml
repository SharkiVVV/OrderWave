<UserControl  xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             Background="RoyalBlue"
             x:Class="OrderWave.Page3">

	<StackPanel>

		<Button x:Name="Buttton" IsEnabled="True" Content="Go to Page 2" HorizontalAlignment="Center" VerticalAlignment="Center" Click="OnNavigateButtonClick"/>

		<StackPanel Width="300" Spacing="10">
		<TextBlock Text="Tables" FontWeight="Bold"/>
		<DataGrid
			ItemsSource="{Binding Orders}"
			AutoGenerateColumns="True"
			GridLinesVisibility="All"
			BorderThickness="1"
			BorderBrush="Gray"
			MaxHeight="150"/>

	</StackPanel>
		
	</StackPanel>

	
</UserControl>
