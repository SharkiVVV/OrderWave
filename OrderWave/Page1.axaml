<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="OrderWave.Page1">

	<StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
		<TextBlock Text="Page 1" HorizontalAlignment="Center"/>
		<TextBox x:Name="TBL" Watermark="Input login" Margin="0,5,0,0" TextChanged="CheckLoginandPassword" Width="400" Height="50" />
		<TextBox x:Name="TBP" Watermark="Input password" Margin="0,5,0,0" TextChanged="CheckLoginandPassword" Width="400" Height="50"/>
		<Button x:Name="Buttton" IsEnabled="False" Content="Go to Page 2" HorizontalAlignment="Center" VerticalAlignment="Center" Click="OnNavigateButtonClick"/>
	</StackPanel>
</UserControl>
